<config xmlns="http://www.ehcache.org/v3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.9.xsd">

    <cache alias="employees">
        <expiry>
            <ttl unit="minutes">5</ttl> <!-- Cache expiration time -->
        </expiry>
        <heap unit="entries">30,000</heap> <!-- Maximum number of entries -->
    </cache>

</config>
